pages = {99--114},
publisher = {Elsevier Inc.},
title = {{An extended protocol for usability validation of medical devices: research design and reference model}},
url = {http://www.sciencedirect.com.proxy.lib.uwaterloo.ca/science/article/pii/S153204641730059X},
volume = {69},
year = {2017}
}

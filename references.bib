journal = {Epidemiology (Cambridge, Mass.)},
keywords = {Epidemiologic Methods,Health Status,Humans,Likelihood Functions,Linear Models,Logistic Models,Models, Statistical,Population,Regression Analysis,Residence Characteristics,Risk Factors},
